generator client {
  provider = "prisma-client-js"
  output   = "../lib/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        String   @id @default(uuid())
  name      String
  image     String
  email     String   @unique
  createdAt DateTime @default(now())
  provider  Provider
}

enum Provider {
  Google
}
